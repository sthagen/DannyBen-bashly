if default_args.any? or default_flags.any?
  = view_marker

  default_args.each do |arg|
    > [[ -n ${args['{{ arg.name }}']:-} ]] || args['{{ arg.name }}']="{{ arg.default }}"
  end

  default_flags.each do |flag|
    > [[ -n ${args['{{ flag.name }}']:-} ]] || args['{{ flag.name }}']="{{ flag.default }}"
  end

  >
end
