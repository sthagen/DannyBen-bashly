---
- :path: spec/tmp/completions.bash
  :content: |
    # download completion                                      -*- shell-script -*-

    # This bash completions script was generated by
    # completely (https://github.com/dannyben/completely)
    # Modifying it manually is not recommended

    _download_completions() {
      local cur=${COMP_WORDS[COMP_CWORD]}
      local compline="${COMP_WORDS[@]:1:$COMP_CWORD-1}"

      case "$compline" in
        *)
          while read; do COMPREPLY+=( "$REPLY" ); done < <( compgen -W "--force --help --version -f -h -v" -- "$cur" )
          ;;

      esac
    } &&
    complete -F _download_completions download

    # ex: filetype=sh
